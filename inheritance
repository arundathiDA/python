class A:
    def Feature1(self):
        print("Feature 1 working")
    def Feature2(self):
        print("Feature 2 working")


#B is a child class /sub class
class B(A):
    def Feature3(self):
        print("Feature 3 working")
    def Feature4(self):
        print("Feature 4 working")

#C is super sub class

class C(B):
    def Feature5(self):
        print("Feature 5 working")


a1 = A()
b1 = B()
c1 = C()


a1.Feature1()
a1.Feature2()
b1.Feature3()
b1.Feature4()
c1.Feature5()
